{
  "name": "todolist",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://igor-tech.github.io/Task-manager",
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@hookform/resolvers": "^3.1.1",
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "^4.11.3",
    "@material-ui/lab": "^4.0.0-alpha.61",
    "@mui/material": "^5.11.0",
    "@mui/styled-engine-sc": "^5.11.0",
    "@reduxjs/toolkit": "^1.9.2",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^20.4.5",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/react-router-dom": "^5.3.3",
    "@types/storybook-react-router": "^1.0.2",
    "@types/uuid": "^8.3.4",
    "axios": "^1.2.1",
    "clsx": "^2.0.0",
    "dlx": "^0.2.1",
    "eslint-config-react-app": "^7.0.1",
    "formik": "^2.2.9",
    "init": "^0.1.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.45.2",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.5.0",
    "react-scripts": "5.0.1",
    "react-toastify": "^9.1.3",
    "redux": "^4.2.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.4.2",
    "storybook": "^7.2.0",
    "styled-components": "^5.3.6",
    "typescript": "^5.1.6",
    "uuid": "^9.0.0",
    "web-vitals": "^2.1.0",
    "zod": "^3.21.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --transformIgnorePatterns \"node_modules/(?!axios)/\"",
    "eject": "react-scripts eject",
    "predeploy": "yarn run build",
    "deploy": "gh-pages -d build",
    "storybook-dev": "start-storybook -p 9009 -s public --no-dll",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "jest:integration": "jest -c integration/jest.config.js",
    "test:integration": "start-server-and-test storybook http-get://localhost:9009 jest:integration",
    "format": "prettier --write src",
    "lint": "eslint --fix src/**/*.{tsx,ts,jsx,js} --no-error-on-unmatched-pattern && stylelint --fix src/{,*/}*.{scss,css} --allow-empty-input"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@it-incubator/eslint-config": "^0.1.3",
    "@it-incubator/prettier-config": "^0.1.2",
    "@it-incubator/stylelint-config": "^0.1.5",
    "@storybook/addon-a11y": "^7.2.0",
    "@storybook/addon-actions": "^6.5.14",
    "@storybook/addon-essentials": "^6.5.14",
    "@storybook/addon-interactions": "^6.5.14",
    "@storybook/addon-links": "^6.5.14",
    "@storybook/builder-webpack5": "^6.5.14",
    "@storybook/manager-webpack5": "^6.5.14",
    "@storybook/node-logger": "^6.5.14",
    "@storybook/preset-create-react-app": "^4.1.2",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^7.2.0",
    "@storybook/testing-library": "^0.0.13",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "babel-plugin-named-exports-order": "^0.0.2",
    "css-loader": "^6.8.1",
    "eslint": "^8.46.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "eslint-plugin-storybook": "^0.6.13",
    "jest-image-snapshot": "^6.1.0",
    "jest-puppeteer": "^6.1.1",
    "prettier": "2.8.7",
    "prop-types": "^15.8.1",
    "puppeteer": "^19.3.0",
    "sass": "^1.64.2",
    "sass-loader": "^13.3.2",
    "start-server-and-test": "^1.14.0",
    "storybook-addon-react-router-v6": "^0.2.1",
    "storybook-react-router": "^1.0.8",
    "style-loader": "^3.3.3",
    "stylelint": "^15.10.2",
    "webpack": "5.88.2"
  }
}
